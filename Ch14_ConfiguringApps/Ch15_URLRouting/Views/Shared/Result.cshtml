@model Result
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link asp-href-include="css/*.css" rel="stylesheet" />
    <title>Routing</title>
</head>
<body class="m-1 p-1">
    <table class="table table-bordered table-striped table-sm">
        <tr>
            <th>Controller:</th>
            <td>@Model.Controller</td>
        </tr>
        <tr>
            <th>Action:</th>
            <td>@Model.Action</td>
        </tr>
        @foreach (string key in Model.Data.Keys)
        {
            <tr>
                <th>@key :</th>
                <td>@Model.Data[key]</td>
            </tr>
        }
    </table>
</body>
</html>